{
  "name": "electron-react_preset",
  "version": "1.0.0",
  "description": "Preset for an Electron application integrated with React.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "vite build",
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\""
  },
  "keywords": [
    "Electron",
    "React",
    "app",
    "preset"
  ],
  "author": "JstOnGit",
  "license": "MIT",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-loader": "^8.2.3",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^38.1.2",
    "electron-builder": "^25.0.0",
    "style-loader": "^4.0.0",
    "vite": "^7.1.6",
    "wait-on": "^7.2.0"
  },
  "build": {
    "files": [
      "dist/**/*",
      "src/**/*",
      "assets/**/*",
      "package.json"
    ],
    "extraResources": [
      "assets/icon.png"
    ]
  }
}
